function ExOh(str) { 
  str = str.toLowerCase();
  var countX = 0;
  var countO = 0;
  var result = "";
  
  for(var i = 0; i < str.length; i++){
  
    if(str[i] === "x"){
      countX += 1;   
    }
    else if (str[i] === "o"){
      countO += 1;
    }
    else {
    continue;
    }
  }
  
  if (countX === countO){
     result = "true";    
  }
  else {
     result = "false";
  }

  return result; 
         
}
