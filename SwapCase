function SwapCase(str) { 

var alphaLower = "abcdefghijklmnopqrstuvwxyz";

var swappedCase = "";


for (var i =0; i<str.length; i++){

if(alphaLower.indexOf(str[i]) >=0){
    swappedCase += str[i].toUpperCase();   
    
}
else {
    swappedCase += str[i].toLowerCase();
    
}

}


  return swappedCase; 
         
}
